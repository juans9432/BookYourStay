<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      spacing="30.0"
      fx:controller="co.edu.uniquindio.bookyourstay.controladores.PanelClienteControlador"
      prefWidth="800"
      prefHeight="600">

    <HBox alignment="CENTER" spacing="10.0">
        <ImageView fitWidth="50.0" preserveRatio="true">
            <!-- Asegúrate de que la ruta sea correcta según la estructura del proyecto -->
            <Image url="@img/clinica.png"/>
        </ImageView>
        <Label text="Panel del cliente">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
    </HBox>

    <TabPane VBox.vgrow="ALWAYS">
        <tabs>
            <Tab text="Reservar" fx:id="tab1" closable="false" />
            <Tab text="Mis Reservas" fx:id="tab2" closable="false" />
            <Tab text="Mi cuenta" fx:id="tab3" closable="false" />
            <Tab text="Recargar" fx:id="tab4" closable="false" />
        </tabs>
    </TabPane>

</VBox>
